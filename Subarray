//Brute force approach to print subarray
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            for(int k=i;k<=j;k++){
                cout<<arr[k]<<" ";
            }cout<<endl;
        }
    }
    return 0;
}
//////////////////OUTPUT////////////////////////////
5
1
5
3
2
6
1 
1 5 
1 5 3 
1 5 3 2 
1 5 3 2 6 
5 
5 3 
5 3 2 
5 3 2 6 
3 
3 2 
3 2 6 
2 
2 6 
6 
